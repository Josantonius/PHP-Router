{
    "name":        "josantonius/router",
    "version":     "1.0.4",
    "type":        "library",
    "description": "Library for handling routes.",
    "keywords": [
        "Router",
        "Routes",
        "PHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "hello@josantonius.com",
            "homepage": "https://josantonius.com",
            "role":     "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/josantonius/php-router"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^5.6 || ^7.0",
        "Josantonius/Url" : "^1.1.2"
    },
    "autoload": {
        "psr-4": {
            "Josantonius\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josantonius\\Router\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
